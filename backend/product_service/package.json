{
  "name": "product_service",
  "version": "1.0.0",
  "description": "A Product Service lambda function",
  "main": "handlers/getProductsList.ts",
  "scripts": {
    "start": "node handlers/getProductsList.ts",
    "build:getProductsList": "esbuild handlers/getProductsList.ts --bundle --platform=node --target=node18 --outfile=dist/getProductsList.js",
    "build:getProductsById": "esbuild handlers/getProductsById.ts --bundle --platform=node --target=node18 --outfile=dist/getProductsById.js",
    "cdk": "cdk",
    "cdk:deploy": "npm run build:getProductsList && npm run build:getProductsById && cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "aws-sdk": "^2.1000.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "esbuild": "^0.25.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.4.4"
  }
}
